{
  "workbench.startupEditor": "newUntitledFile",
  "window.menuBarVisibility": "toggle",
  "files.autoSave": "on",
  "editor.fontSize": 13,
  "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true,
      "node_modules/**": true
  },
  "editor.fontFamily": "Fira Code, Arial, Consolas, 'Courier New', monospace",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.letterSpacing": 0.6,
  "editor.lineHeight": 19,
  "editor.mouseWheelScrollSensitivity": 1,
  "window.zoomLevel": 1,
  "zenMode.hideStatusBar": false,
  "window.zoomLevel": 0.5
}
